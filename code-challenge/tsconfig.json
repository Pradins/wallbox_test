{
  "compilerOptions": {
    "noEmit": true,
    "esModuleInterop": true,
    "target": "ES2015",
    "module": "commonjs",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "strictNullChecks": true
  },
  "exclude": ["node_modules"]
}
